int getLineLength(char s[], int delim)
{
	int c, i, counter;
	
	counter = 0;
	for (int j = 0; ((c = getchar())!= EOF) && c != "\n"; j = j + 1)
	{
		s[i] = c;
		counter = counter + 1;
	}
}
